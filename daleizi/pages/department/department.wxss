page{background-color: #fff;}
video{
    width: 100%;
    height: 350rpx;
}
.choiceDepartment{
    padding: 40rpx 30rpx 10rpx;
    display: flex;
    flex-wrap: wrap;
}
.department{
    padding: 20rpx 30rpx;
    border: 1rpx solid #53c7c0;
    border-radius: 36rpx;
    margin-bottom: 30rpx;
    text-align: center;
    margin-right: 20rpx;
    font-size: 26rpx;
    color: #777;
}
.divide{
    width: 100%;
    height: 20rpx;
    background-color: #f7f7f7;
    border-top: 1rpx solid #ebebeb;
    border-bottom: 1rpx solid #ebebeb;
}
.introduceYykoo{
    width: 100%;
    border-bottom:1rpx solid #ebebeb;
}
.about{
    border-bottom:1rpx solid #ebebeb;
}
.about,.doubt{
    margin-left: 30rpx;
    position: relative;
}
.introduceIcon{
    width: 50rpx;
    height: 50rpx;
    display: inline-block;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}
.introduceText{
    display: block;
    padding: 30rpx 46rpx 30rpx 80rpx;
    font-size: 30rpx;
    color: #999;
}
.introduceNext{
    width: 16rpx;
    height: 27rpx;
    position: absolute;
    top: 50%;
    right: 30rpx;
    transform: translateY(-50%);
}