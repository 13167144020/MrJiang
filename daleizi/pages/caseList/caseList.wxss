page{
    padding-top: 20rpx;
}
.caseHistory{
    width: 100%;
    margin-bottom: 20rpx;
    background-color: #fff;
    border-bottom: 1rpx solid #ebebeb;
    border-top: 1rpx solid #ebebeb;
    padding-left: 30rpx;
}
.caseInfo{
    border-bottom: 1rpx solid #ebebeb;
    padding-right: 30rpx;
}
.essentialInfo{
    overflow: hidden;
    padding-top: 30rpx;
}
.caseNema{
    font-size: 40rpx;
    color: #66cdc7;
    display: inline-block;
    margin-right: 30rpx;
}
.nexus{
    font-size: 30rpx;
    color: #666;
}
.time{
    font-size: 26rpx;
    color: #999;
    float: right;
    line-height: 50rpx;
}
.gender,.age,.region{
    font-size: 26rpx;
    color: #666;
    display: inline-block;
    padding:14px 0 30rpx;
}
.gender,.age{
    margin-right: 30rpx;
}

.caseDepict{
    padding-right: 110rpx;
    position: relative;
}
.caseNext{
    width: 20rpx;
    height: 30rpx;
    position: absolute;
    top: 50%;
    right: 30rpx;
    transform: translateY(-50%);
}
.depictTitle{
    font-size: 30rpx;
    color: #333;
    padding-top: 30rpx;
}
.depictTxt{
    width: 100%;
    display: block;
    padding-right: 30rpx;
    font-size: 26rpx;
    color: #666;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding: 30rpx 0 40rpx;
}
.newAdd{
    width: 100%;
    background-color: #fff;
    border-top:1rpx solid #ebebeb;
    border-bottom: 1rpx solid #ebebeb;
    padding: 50rpx;
    
}
.addBlock{
    border: 1rpx dashed #ebebeb;
    text-align: center; 
    padding: 80rpx 0 50rpx;
}
.addPic{
    width: 60rpx;
    height: 60rpx;
    
}
.addTxt{
    display: block;
    font-size: 26rpx;
    color: #999;
     padding-top: 50rpx;
}
.red{
    color: #fd6e6e;
}
