.detail{
    width: 100%;
    height: 380rpx;
    background-color: #00c1b9;
    padding: 24rpx 30rpx 0;
}
.info{
    overflow: hidden;
}
.head{
    width: 198rpx;
    height: 198rpx;
    background: rgba(255, 255, 255, 0.2);
    border-radius: 50%;
    text-align: center;
    float: left;
}
.headPic{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    border: 10rpx solid rgba(255, 255, 255, 0.3);
    margin: 14rpx auto;
}
.infoTxt{
    color: #fff;
    float: left;
    margin:24rpx 0 24rpx 30rpx;
}
.doctorName{
    font-size: 36rpx;
    font-weight: bold;
    margin-right: 30rpx;
}
.physician,.activeTime{
    font-size: 26rpx;
    
}
.hospital{
    display:block;
    font-size: 26rpx;
    padding: 10rpx 0;
}
.hospitalLine{
    margin: 0 14rpx;
}
.service{
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    color: #fff;
    padding-top:18rpx;
}
.serviceTxt{
    text-align: center;
    position: relative;
}
.nmb{
    display: block;
    font-size: 42rpx;
    font-weight: bold;
}
.nmbTitle{
    font-size: 26rpx;
}
.nmbline{
    position: absolute;
    top: 50%;
    right: -80rpx;
    transform: translateY(-50%);
}
.price{
    padding:30rpx;
    display: flex;
    justify-content: space-between;
}
.consultingFee{
    font-size: 26rpx;
    color: #999;
    padding-left: 40rpx;
}
.red{
    color:#ff4e4e; 
}
.money{
    color:#ff4e4e; 
    font-size: 56rpx;
    line-height: 96rpx;
}
.btn{
    width: 390rpx;
    height: 96rpx;
    background: #ff4e4e;
    border-radius: 10rpx;
    
}
.doctorDetail{
    background-color: #fff;
    border-bottom:1rpx solid #ebebeb;
    margin-bottom:20rpx;
}
.btnIcon{
    width: 54rpx;
    height: 42rpx;
    padding: 27rpx 30rpx 27rpx 60rpx; 
    float: left;
}
.btnTect{
    font-size: 32rpx;
    color: #fff;
    line-height: 96rpx;
}
.advantage{
    width: 100%;
    border-top:1rpx solid #ebebeb;
    padding: 40rpx 0;
    display: flex;
    justify-content: center;
}
.sendBackIcon,.noLimitIcon{
    width: 36rpx;
    height: 36rpx;
    margin-right: 22rpx;
}
.advantageLine{
    color: #ebebeb;
    padding: 0 22rpx;
}
.advantageTxt{
    font-size: 30rpx;
    color: #999;
}
.advantageOne,.advantageTwo{
    font-size: 24rpx;
    color: #999;
    display: block;
    padding:0 30rpx 50rpx 30rpx;
    line-height: 38rpx;
}
.curriculumvVitae{
    width: 100%;
    border-top:1rpx solid #ebebeb; 
    background-color: #fff;
}
.cv_title{
    font-size: 30rpx;
    color: #999;
    text-align: center;
    border-bottom:1rpx solid #ebebeb;
    line-height: 90rpx;
}
.cv_info{
    padding: 0 30rpx;
}
video{
    width: 100%;
    height: 340rpx;
    border-radius: 10rpx;
    margin: 30rpx 0 60rpx;
}
.contentTitle{
    font-size: 32rpx;
    color: #333;
    text-align: center;
    margin-bottom: 40rpx;
    position: relative;
}
.contentTitle::after{
    content:"";
    display: block;
    width: 10rpx;
    height: 10rpx;
    background: #00c1b9;
    transform: rotate(45deg) translateY(-50%);
    position: absolute;
    top: 50%;
    right: 230rpx;
}
.contentTitle::before{
    content:"";
    display: block;
    width: 10rpx;
    height: 10rpx;
    background: #00c1b9;
    transform: rotate(45deg) translateY(-50%);
    position: absolute;
    top: 50%;
    left: 220rpx;
}
.contentText{
    display: block;
    font-size: 26rpx;
    color: #666;
    padding-bottom: 46rpx;
}