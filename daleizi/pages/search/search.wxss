/* pages/search/search.wxss */

page {
  background: #f7f7f7;
}

.search {
  background: #fff;
  height: 90rpx;
  border-bottom: 1rpx solid #ebebeb;
}

.searchInput {
  margin: 17rpx 0 17rpx 30rpx;
  height: 56rpx;
  border-radius: 8rpx;
  background: url('http://hybrid.yykaoo.com/imgs/search.jpg') no-repeat 18rpx center #eaefef;
  background-size: auto 30rpx;
  padding-left: 66rpx;
}

.searchInput input {
  display: block;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  color: #333;
}

.undo {
  color: #73c4bf;
  font-size: 34rpx;
  line-height: 90rpx;
  padding: 0 34rpx;
}
